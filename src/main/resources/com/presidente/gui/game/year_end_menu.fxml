<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="vBox" styleClass="menu" alignment="TOP_CENTER" prefHeight="350.0" prefWidth="500.0"
      spacing="25.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="com.presidente.display.controller.game.YearEndMenuController">
    <Label alignment="CENTER" prefHeight="44.0" prefWidth="464.0"
           text="L'année est terminé il est temps de prendre vos décisions annuel" textAlignment="CENTER"
           wrapText="true">
        <font>
            <Font name="Stencil" size="18.0"/>
        </font>
    </Label>
    <VBox alignment="CENTER" prefHeight="280.0" prefWidth="500.0" spacing="40.0">
        <Button fx:id="buyFoodButton" alignment="CENTER" mnemonicParsing="false" onAction="#buyFood" prefWidth="200.0"
                text="Acheter des vivres" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Stencil" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="corruptButton" alignment="CENTER" layoutX="10.0" layoutY="10.0" mnemonicParsing="false"
                onAction="#corruptFaction" prefWidth="200.0" text="Soudoyer une faction" textAlignment="CENTER"
                wrapText="true">
            <font>
                <Font name="Stencil" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="continuePlaying" alignment="CENTER" layoutX="10.0" layoutY="10.0" mnemonicParsing="false"
                onAction="#continuePlaying" prefWidth="200.0" text="Poursuivre la partie" wrapText="true">
            <font>
                <Font name="Stencil" size="14.0"/>
            </font>
        </Button>
    </VBox>
</VBox>
