   <children>
      <Button fx:id="createButton" disable="true" layoutX="113.0" layoutY="508.0" mnemonicParsing="false" onAction="#startGame" text="Commencer la partie" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="100.0">
