<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="vBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" spacing="40.0"
      styleClass="menu" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="com.presidente.display.controller.menu.LoadGameController">
    <ListView fx:id="saveList" prefHeight="353.0" prefWidth="584.0">
        <VBox.margin>
            <Insets left="100.0" right="100.0"/>
        </VBox.margin>
    </ListView>
    <HBox alignment="CENTER" spacing="20.0">
        <Button fx:id="loadButton" disable="true" mnemonicParsing="false" onAction="#loadSave" prefWidth="150.0"
                text="Charger la sauvegarde"/>
        <Button mnemonicParsing="false" onAction="#backToPrevious" prefWidth="150.0" text="Retour"/>
    </HBox>
</VBox>
